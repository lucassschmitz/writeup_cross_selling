@article{boehm_intermediation_2024,
	title = {Intermediation, {Choice} {Frictions}, and {Adverse} {Selection}: {Evidence} from the {Chilean} {Pension} {Market}},
	abstract = {This paper analyzes the consumer-welfare effects of intermediaries in a pension and annuity market with adverse selection. Intermediaries provide advice, helping individuals improve decisions when understanding products is complex and costly, but may introduce distortions due to agency problems. In an insurance market, intermediary effects on choices can impact adverse selection and, through it, prices. I document the importance of intermediation and its connection to adverse selection in the Chilean pension market, where products are complex and intermediaries have a ﬁnancial incentive to steer consumers toward annuities. To quantify the effects of potential intermediary regulations, I develop and estimate a dynamic demand model that includes life-cycle decisions, product, and intermediation choices. I ﬁnd intermediaries have the potential to improve welfare: retirees would give up around 250 USD a year to eliminate frictions in product choices. Despite intermediaries steering a majority of their customers into annuities, a ban on intermediation is approximately consumer-welfare neutral. The variety of annuities allows intermediaries to recommend close substitutes to the outside option, limiting the harm from misaligned incentives. Decision costs without intermediaries and annuity price increases due to adverse selection erode any gains from a ban. In light of policy concerns regarding the role of intermediaries, my results highlight the potential value provided by advisors – even with biased incentives – when choices are complex and stakes are high.},
	language = {en},
	author = {Boehm, Eduard},
	year = {2024},
}

