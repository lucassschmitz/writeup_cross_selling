@article{cai_information_nodate,
	title = {Information {Disclosure} and {Efficiency} in {Dynamic} {Price} {Negotiations}: {An} {Experimental} and {Structural} {Analysis}},
	abstract = {This paper examines the causal effects of information disclosure policies on dynamic price negotiations. While information disclosure facilitates informed decision-making by enabling users to reveal private information, it also induces strategic signaling and price discrimination that can undermine market efficiency. I use a large-scale panel experiment on an on-demand delivery platform where drivers offer delivery prices to shippers (customers). The platform randomly assigns cities either to a treatment group—where drivers observe the lowest price previously rejected by shippers (full disclosure)—or to a control group, where no such information is disclosed (no disclosure). I find that full disclosure significantly increases shippers’ rejection rates, holding offer prices fixed. In response, drivers lower their offers on average and price-discriminate based on shippers’ rejection records. Full disclosure decreases order transaction prices and lengthens transaction duration. To quantify equilibrium and welfare impacts of information disclosure policies, I develop a dynamic negotiation model, estimate its primitives using treatment data, and validate the model on holdout control data. I find that full disclosure benefits low-value shippers—who signal low valuations through strategic rejections, obtaining lower prices despite longer waits—while harming high-value shippers through driver price discrimination. Counterfactual analyses show that a class of partial disclosure policies—disclosing only the lowest shipper-rejected price below a threshold—achieve Pareto improvements in user welfare and platform revenue by mitigating negative signaling externalities, reducing driver markups, and enhancing market efficiency. Comparative statics show that the disclosure thresholds that maximize driver profits, platform revenue, and social welfare rise as shippers become less patient, as exit rates increase, or as drivers arrive less often. Targeting market primitives to optimize market-specific disclosure thresholds increases social welfare by 13.3\% and 14.1\% relative to the no disclosure and full disclosure benchmarks, respectively.},
	language = {en},
	author = {Cai, Gaoyang},
}

@article{boche_efficient_nodate,
	title = {Efficient {Health} {Insurance} with {Moral} {Hazard}, {Adverse} {Selection}, and {Evolving} {Risk}},
	abstract = {I study consumer-optimal health insurance under evolving risk with adverse selection and ex-post moral hazard. I show that, under a simplification of incentive constraints known as the first-order approach (FOA), optimal linear coverage will be set higher than would be efficient without adverse selection. This distortion relaxes upward incentive constraints for lower-risk consumers, thereby allowing for greater cross-subsidization from lower-risk to higher-risk consumers, which is important for insuring dynamic risk. I then characterize the optimal menu of nonlinear coinsurance schedules under the FOA. Results inform the design of both private-sector dynamic health insurance contracts and social health insurance programs with a participation mandate administered by governments or employers.},
	language = {en},
	author = {Boche, Alex},
}

