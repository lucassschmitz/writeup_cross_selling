	keywords = {Actuarial Studies, Analysis of Health Care Markets, Analysis of Health Care Markets, Asymmetric and Private Information, Asymmetric and Private Information; Mechanism Design, Insurance Companies, Insurance; Insurance Companies; Actuarial Studies, Mechanism Design, Insurance},
	pages = {938--958},
}

@article{grunewald_auto_nodate,
	title = {Auto {Dealer} {Loan} {Intermediation}: {Consumer} {Behavior} and {Competitive} {Eﬀects}},
	abstract = {This paper studies the intermediation of auto loans through auto dealers. Auto lenders incentivize dealers to use their soft “showroom” information about consumers to engage in price discrimination by charging discretionary interest rate markups. Using new and uniquely rich administrative data, we show that markups have large eﬀects on interest rates, vary widely across consumers, and are very poorly predicted by variables lenders have access to. We estimate a model that allows us to quantify the eﬀects of banning discretionary dealer markups. The model accounts for equilibrium adjustments in dealer, lender, and consumer behavior. Banning dealer markups decreases average transaction prices, and is especially beneﬁcial for consumers with lower income and less ﬁnancial sophistication.},
	language = {en},
	author = {Grunewald, Andreas and Lanning, Jonathan and Low, David and Salz, Tobias},
}

@misc{luttmer_failures_2025,
	type = {Working {Paper}},
	series = {Working {Paper} {Series}},
	title = {Failures of {Contingent} {Reasoning} in {Annuitization} {Decisions}},
	url = {https://www.nber.org/papers/w33654},
	doi = {10.3386/w33654},
	abstract = {This paper studies psychological biases in take-up of annuities, using an incentivized experiment with a probability-based sample (N = 3,038). Choosing an annuity was payoff-maximizing in the experiment at all prices, but take-up was incomplete and price elastic. Reformulating decisions as insurance against a “bad” outcome rather than insurance against “longevity risk” did not increase take-up. Instead, we find substantial failures of contingent reasoning: participants under-appreciated how annuitization mitigated the need for less-efficient means of saving for retirement. Increasing the salience of the interaction with savings decisions, or eliminating the need to think through this interaction altogether, substantially increased annuity take-up.},
	urldate = {2025-11-14},
	publisher = {National Bureau of Economic Research},
	author = {Luttmer, Erzo F.P. and de Oliveira, Priscila and Taubinsky, Dmitry},
