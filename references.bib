	author = {Agarwal, Sumit and Grigsby, John and Hortaçsu, Ali and Matvos, Gregor and Seru, Amit and Yao, Vincent},
	year = {2024},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.3982/ECTA18554},
	keywords = {Mortgages, screening, search},
	pages = {1195--1231},
}

