@article{gonzales_investment_2023,
	title = {The {Investment} {Effects} of {Market} {Integration}: {Evidence} {From} {Renewable} {Energy} {Expansion} in {Chile}},
	volume = {91},
	copyright = {© 2023 The Authors. Econometrica published by John Wiley \& Sons Ltd on behalf of The Econometric Society},
	issn = {1468-0262},
	shorttitle = {The {Investment} {Effects} of {Market} {Integration}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.3982/ECTA20769},
	doi = {10.3982/ECTA20769},
	abstract = {We study the investment effects of market integration on renewable energy expansion. Our theory highlights that market integration not only improves allocative efficiency by gains from trade but also incentivizes new investment in renewable power plants. To test our theoretical predictions, we examine how recent grid expansions in the Chilean electricity market changed electricity production, wholesale prices, generation costs, and renewable investments. We then build a structural model of power plant entry to quantify the impact of market integration with and without the investment effects. We find that the market integration in Chile increased solar generation by around 180\%, saved generation costs by 8\%, and reduced carbon emissions by 5\%. A substantial amount of renewable entry would not have occurred in the absence of market integration. Our findings suggest that ignoring these investment effects would substantially understate the benefits of market integration and its important role in expanding renewable energy.},
	language = {en},
	number = {5},
	journal = {Econometrica},
	author = {Gonzales, Luis E. and Ito, Koichiro and Reguant, Mar},
	year = {2023},
	keywords = {Renewable energy, market integration, transmission expansion, wholesale electricity markets},
	pages = {1659--1693},
}

@article{brugues_bank_2024,
	title = {Bank market power and credit allocation},
	abstract = {We study how bank competition aﬀects commercial lending, extending the ﬁndings in Brugués and De Simone (2024). We ﬁnd that 26\% of observed markups are due to joint proﬁt maximization and that moving to Bertrand-Nash would reduce equilibrium prices by 17\%, increase loan use by 21\% (intensive margin), and increase overall credit demand by 13\% (extensive margin). These distortions vary greatly by borrower characteristics and dwarf those of ﬁnancial transaction taxes. Through partial equilibrium instrumental variable regressions, we ﬁnd large eﬀects on ﬁrm size and productivity. We aggregate this partial equilibrium eﬀect through a general equilibrium model of ﬁrm dynamics to measure the dynamic eﬀects of credit and ﬁrm growth. Overall, our ﬁndings suggest that the lack of competition in banking has ﬁrst-order implications for credit and misallocation.},
	language = {en},
	author = {Brugués, Felipe and Simone, Rebecca De},
	year = {2024},
}

@article{petersen_benefits_1994,
	title = {The {Benefits} of {Lending} {Relationships}: {Evidence} from {Small} {Business} {Data}},
	volume = {49},
	copyright = {© 1994 the American Finance Association},
	issn = {1540-6261},
	shorttitle = {The {Benefits} of {Lending} {Relationships}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1540-6261.1994.tb04418.x},
	doi = {10.1111/j.1540-6261.1994.tb04418.x},
	abstract = {This paper empirically examines how ties between a firm and its creditors affect the availability and cost of funds to the firm. We analyze data collected in a survey of small firms by the Small Business Administration. The primary benefit of building close ties with an institutional creditor is that the availability of financing increases. We find smaller effects on the price of credit. Attempts to widen the circle of relationships by borrowing from multiple lenders increases the price and reduces the availability of credit. In sum, relationships are valuable and appear to operate more through quantities rather than prices.},
	language = {en},
	number = {1},
	urldate = {2025-10-29},
