	keywords = {Clean air act, dynamic games, environmental regulation, portland cement},
	pages = {1019--1061},
}

@article{kortum_optimal_nodate,
	title = {Optimal {Unilateral} {Carbon} {Policy}},
	abstract = {Climate policy by a coalition of countries can shift activities—extraction, production, and consumption—to regions outside the coalition. We build a general-equilibrium model of trade and carbon externalities to derive a coalition’s optimal Pareto-improving policy in such an environment. The policy can be implemented through: (i) a tax on fossil-fuel extraction at a rate equal to the global marginal harm from carbon emissions, (ii) a tax on imports of energy and goods, and a rebate of the tax on exports of energy but not goods, all at a lower rate per unit of carbon than the extraction tax rate, and (iii) a good-specific export subsidy. This combination of taxes and subsidies exploits international trade to expand the policy’s reach. It promotes energy efficient production and limits leakage by taxing the carbon content of imports of goods, by encouraging exports of goods, and by controlling the energy price in the non-taxing region by balancing supplyside and demand-side taxes. We use a quantitative version of the model to illustrate the gains achieved by the optimal policy and simpler variants. The optimal policy generates first-order welfare improvements over current and proposed climate policies.},
	language = {en},
	author = {Kortum, Samuel and Weisbach, David A},
}

