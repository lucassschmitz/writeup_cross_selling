	abstract = {This paper provides a model of firm and industry dynamics that allows for entry, exit and firm-specific uncertainty generating variability in the fortunes of firms. It focuses on the impact of uncertainty arising from investment in research and exploration-type processes. It analyses the behaviour of individual firms exploring profit opportunities in an evolving market place and derives optimal policies, including exit, in this environment. Then it adds an entry process and aggregates the optimal behaviour of all firms, including potential entrants, into a rational expectations, Markov-perfect industry equilibrium, and proves ergodicity of the equilibrium process. Numerical examples are used to illustrate the more detailed characteristics of the stochastic process generating industry structures that result from this equilibrium.},
	number = {1},
	urldate = {2025-08-25},
	journal = {The Review of Economic Studies},
	publisher = {[Oxford University Press, Review of Economic Studies, Ltd.]},
	author = {Ericson, Richard and Pakes, Ariel},
	year = {1995},
	pages = {53--82},
}

@article{brown_endogenous_2024,
	title = {Endogenous {Information} and {Simplifying} {Insurance} {Choice}},
	volume = {92},
	issn = {0012-9682},
	url = {https://www.econometricsociety.org/doi/10.3982/ECTA18555},
	doi = {10.3982/ECTA18555},
	abstract = {In markets with complicated products, individuals may choose how much time and effort to spend understanding and comparing alternatives. Focusing on insurance choice, we ﬁnd evidence consistent with individuals acquiring more information when there are larger consequences from making an uninformed choice. Building on the rational inattention literature, we develop and estimate a parsimonious demand model in which individuals choose how much to research difﬁcult-to-observe characteristics. We use our estimates to evaluate policies that simplify choice. Reducing the number of plans can raise welfare through improved choice as well as savings in information costs. Capping out-of-pocket costs generates larger welfare gains than standard models. The empirical model can be applied to other settings to examine the regulation of complex products.},
	language = {en},
	number = {3},
	urldate = {2025-08-15},
	journal = {Econometrica},
	author = {Brown, Zach Y. and Jeon, Jihye},
	year = {2024},
	keywords = {Insurance, information frictions, rational inattention},
	pages = {881--911},
}

@article{amiti_trade_2007,
